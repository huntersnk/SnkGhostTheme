{{#foreach posts}}
<section id="{{comment_id}}" class="{{post_class}}">
	<h3 class="post-title"><a href="{{url}}">{{title}}</a><span class="separator"></span><span style="float:right;">{{date published_at format='YYYY.MM.DD'}}<span>{{#if featured}}<span class="separator"> &middot; </span><i class="fa fa-star-o feature-star"></i>{{/if}}</h3>
</section>
{{/foreach}}